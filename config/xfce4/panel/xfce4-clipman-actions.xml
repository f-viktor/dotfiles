<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action>
		<name>Image</name>
		<regex>(http|ftp).+\.(jpg|png|gif)</regex>
		<group>0</group>
		<commands>
			<command>
				<name>Edit with Gimp</name>
				<exec>gimp &quot;\0&quot;</exec>
			</command>
		</commands>
	</action>
	<action>
		<name>Long URL</name>
		<regex>http://[^\s]{120,}</regex>
		<group>0</group>
		<commands>
			<command>
				<name>Shrink the URL</name>
				<exec>exo-open http://tinyurl.com/create.php?url=\0</exec>
			</command>
		</commands>
	</action>
	<action>
		<name>&quot;Quote&quot;</name>
		<regex>^quote((.|\n)+)</regex>
		<group>0</group>
		<commands>
			<command>
				<name>Remove newlines</name>
				<exec>/home/pepe/.config/xfce4/panel/newliner &quot;\0&quot;</exec>
			</command>
		</commands>
	</action>
</actions>
